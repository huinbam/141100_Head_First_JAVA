public class PoolPuzzleOne {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 0;
		while (x < 3) {
			System.out.print("a");
			if (x < 1) {
				System.out.print(" ");
			}
			if (x < 1)
			{
				System.out.print("noise");
			}
				System.out.println("");
			x++;
		}

	}
}
