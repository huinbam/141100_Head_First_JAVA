public class PoolPuzzleOne {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 0;
		while (x < 3) {
			System.out.print("a");
			if (x < 1) {
				System.out.print(" ");
			}
			if (x > 0) {
				System.out.print("n");
			}
			if (x == 1) {
				System.out.print("noys");
				x = x + 2;
			}
			if (x >2) {
				System.out.print("noise");
			}
			System.out.println("");
			x++;
		}

	}
}
