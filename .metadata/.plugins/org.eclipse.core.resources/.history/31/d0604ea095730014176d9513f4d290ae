package ch02;

class Movie {

	String title;
	String genre;
	int rating;

	void playIt() {
		System.out.println("영화를 상영합니다.");
	}
}

public class MovieTestDrive {
	public static void main(String[] args){
		Movie one = new Movie();
		one.title = "주식과 함께 사라지다";
		one.genre="비극";
		one.rating=-2;
		
	}
}
